{
  "name": "aframe-orbit-controls",
  "version": "1.3.1",
  "description": "Orbit controls for A-Frame.",
  "module": "index.js",
  "main": "dist/aframe-orbit-controls.min.js",
  "scripts": {
    "build": "webpack",
    "dev": "cross-env NODE_ENV=production webpack serve",
    "dist": "cross-env NODE_ENV=production webpack",
    "lint": "semistandard -v | snazzy",
    "prepublish": "npm run dist",
    "ghpages": "ghpages",
    "start": "npm run dev",
    "test": "karma start ./tests/karma.conf.js",
    "test:firefox": "karma start ./tests/karma.conf.js --browsers Firefox",
    "test:chrome": "karma start ./tests/karma.conf.js --browsers Chrome"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/supermedium/superframe.git"
  },
  "keywords": [
    "aframe",
    "aframe-component",
    "aframe-vr",
    "vr",
    "mozvr",
    "webvr",
    "orbit-controls"
  ],
  "author": "Kevin Ngo <me@ngokevin.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/supermedium/superframe/issues"
  },
  "homepage": "https://github.com/supermedium/superframe#readme",
  "devDependencies": {
    "aframe": "*",
    "chai": "^4.3.6",
    "chai-shallow-deep-equal": "^1.4.0",
    "cross-env": "^7.0.3",
    "ghpages": "^0.0.8",
    "karma": "^6.4.0",
    "karma-webpack": "^5.0.0",
    "karma-chai-shallow-deep-equal": "0.0.4",
    "karma-chrome-launcher": "^3.1.1",
    "karma-env-preprocessor": "^0.1.1",
    "karma-firefox-launcher": "^2.1.2",
    "karma-mocha": "^2.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sinon-chai": "^2.0.2",
    "mocha": "^10.0.0",
    "randomcolor": "^0.4.4",
    "semistandard": "^8.0.0",
    "shelljs": "^0.7.0",
    "sinon": ">=1.17.5",
    "sinon-chai": "^2.8.0",
    "shx": "^0.1.1",
    "snazzy": "^4.0.0",
    "webpack": "^5.79.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.13.2"
  },
  "semistandard": {
    "globals": [
      "AFRAME",
      "THREE"
    ],
    "ignore": [
      "examples/build.js",
      "dist/**"
    ]
  }
}
